{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/uploads/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" },
      "dest": "/uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
